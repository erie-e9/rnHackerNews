{
  "name": "rnhackernews",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint --fix . --c .js,.jsx,.ts,.tsx,",
    "start": "npx react-native start --reset-cache",
    "test": "jest",
    "type-check": "tsc",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "format": "prettier --write .",
    "depcheck": "depcheck",
    "prepare": "husky install",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "androidbuild": "react-native run-android --variant=release --verbose",
    "android": "rm -rf node_modules/ && rm -rf yarn.lock && yarn && cd android && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native start --reset-cache",
    "ios": "rm -rf /Users/erictorresandrade/Caches/CocoaPods && rm -rf node_modules/ && rm -rf yarn.lock && yarn && cd ios && pod deintegrate && pod setup && pod cache clean --all && rm -rf Pods/ build/ Podfile.lock && bundle install && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --repo-update && cd .. && npx react-native start --reset-cache",
    "a": "yarn && cd android && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native start --reset-cache",
    "i": "yarn && cd ios && bundle install && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --repo-update && cd .. && npx react-native start --reset-cache",
    "aabi": "cd android && adb uninstall com.hackernews && cd app && rm -rf .cxx && cd .. && ./gradlew clean && ./gradlew --stop && cd .. && npx react-native run-android --active-arch-only",
    "analyze:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output ./android-release.bundle --sourcemap-output ./android-release.bundle.map --reset-cache",
    "analyze:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ./ios-release.bundle --sourcemap-output ./ios-release.bundle.map --reset-cache",
    "check-dependencies": "npx rnx-align-deps",
    "fix-dependencies": "npx rnx-align-deps --write"
  },
  "dependencies": {
    "@notifee/react-native": "^9.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.3.0",
    "@shopify/flash-list": "^1.7.1",
    "date-fns": "^4.1.0",
    "i18next": "^23.16.2",
    "lodash": "^4.17.21",
    "lottie-ios": "4.4.1",
    "lottie-react-native": "^6.7.2",
    "react": "18.2.0",
    "react-i18next": "^15.1.0",
    "react-native": "0.74.0",
    "react-native-background-fetch": "^4.2.5",
    "react-native-device-info": "^11.1.0",
    "react-native-gesture-handler": "2.18.1",
    "react-native-localize": "^3.2.1",
    "react-native-lottie-splash-screen": "^1.1.2",
    "react-native-mmkv": "3.0.1",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.11.1",
    "react-native-screens": "3.31.1",
    "react-native-webview": "^13.12.3",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native/babel-preset": "0.74.86",
    "@react-native/eslint-config": "0.74.86",
    "@react-native/metro-config": "0.74.86",
    "@react-navigation/devtools": "^6.0.27",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.2",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.12",
    "@types/lodash.memoize": "^4.1.9",
    "@types/metro-config": "^0.76.3",
    "@types/node": "^22.0.2",
    "@types/react": "^18.3.3",
    "@types/react-test-renderer": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@types/styled-components-react-native": "^5.2.5",
    "babel-jest": "^29.7.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "dotenv": "^16.4.5",
    "eslint": "^8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-ft-flow": "^3.0.11",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-unused-imports": "^4.0.1",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "patch-package": "^8.0.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.3.1",
    "stylelint": "^16.8.1",
    "stylelint-react-native": "^2.7.0",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
